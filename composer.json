{
  "name": "gustavokataoka/sb2plugin",
  "description": "A CakePHP3 Theme based on SB Admin 2 as a plugin.",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "theme"
  ],
  "homepage": "https://github.com/gustavokataoka/Sb2plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Kataoka",
      "homepage": "https://github.com/gustavokataoka",
      "role": "Author"
    }
  ],
  "require": {
    "cakephp/cakephp": ">=3.2.0 <4.0"
  },
  "autoload": {
    "psr-4": {
      "Sb2plugin\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "bower install"
    ],
    "post-update-cmd": [
      "bower install"
    ]
  }
}